package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {
		String name = "kamal";

		int i = name.length() - 1;
		while (i >= 0) {
			System.out.print(name.charAt(i--));

		}
		System.out.println();
		char c = 'a';
		while (c <= 'z') {
			System.out.print(c++ + " ");

		}
//		System.out.println();
//		char c1 = 'z';
//		while (c1 >= 'a') {
//			System.out.print(c1-- + " ");
//			if (c1 != 'a' - 1) {
//				System.out.print(",");
//				System.out.println();
//			}

		System.out.println();
		int a1 = 1;
		while (a1 <= 20) {
			System.out.print(a1++);
			if (a1 % 2 == 0) {
				System.out.print("-");

			} else {
				System.out.print("");

			}
			System.out.println();
			for (int t = 1; t <= 20; t++) {
				System.out.print(t);
			}
		
		}

	}

}
