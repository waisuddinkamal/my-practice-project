package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {
		String name ="kamal";
		
		int i=name.length()-1;
		while (i>=0) {
			System.out.print(name.charAt(i--));
		
		}
		System.out.println();
		char c = 'a';
		while (c<='z') {
			System.out.print(c++ +" ");
			
		}
	}
}
