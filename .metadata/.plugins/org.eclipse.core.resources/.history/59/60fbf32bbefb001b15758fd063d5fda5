package ClassPractice;

import java.util.Iterator;

public class PracticeAndClean {

	public static void main(String[] args) {

		for (int i = 1; i < 7; i++) {
			for (int j = 0; j < i; j++) {
				System.out.print("*");
			}

			System.out.println();

		}
		for (int i = 1; i > 7; i--) {
			for (int j = 0; j < i; j--) {
				
			}
			System.out.println("*");
			
		}
		System.out.println();

	}
}
