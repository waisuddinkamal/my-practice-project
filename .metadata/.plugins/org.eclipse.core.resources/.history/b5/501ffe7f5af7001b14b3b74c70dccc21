package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		String n = "kamal";
		int i = n.length()-1;
		while (i>=0) {
			System.out.println(n.charAt(i--));
			
		}
		System.out.println();
	
		String name = "kamal";
		for(int n = 4;n>=0;) {
			System.out.print(name.charAt(n--));
		}
		

	}

}
