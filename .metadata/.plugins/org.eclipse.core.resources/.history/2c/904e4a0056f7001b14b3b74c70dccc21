package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		//

		int t = 2;
		for (int i = 1; i <= 10; i++) {
			System.out.println(i + "*" + t + "=" + t * i);
		}
		
		String name = "kabul";
		
		int i = name.length();
		while (i<=name.length()-1) {
			System.out.print(name.charAt(i--));
		}
		System.out.println();

	}

}
