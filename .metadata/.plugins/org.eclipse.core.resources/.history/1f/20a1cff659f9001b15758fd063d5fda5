package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		int i = 10;
		while (i>=0) {
			System.out.println(i);
			i--;
		}
		System.out.println();
		
		int t = 0;
		while (t<=10) {
			System.out.println(t);
			t++;
		}
		
		
		int r = 10;
		while (r>=0) {
			System.out.println(r--);
			
		}
	}
}