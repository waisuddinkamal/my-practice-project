package ClassPractice;

import java.util.Iterator;

public class PracticeAndClean {

	public static void main(String[] args) {
		for (int w = 0; w < 3; w++) {
		
		
		for (int i = 1; i < 6; i++) {
			for (int j = 0; j < i; j++) {
				System.out.print("*");
			}

			System.out.println();

		}
		for (int i = 6; i >= 0; i--) {
			for (int j = i; j > 0; j--) {
				System.out.print("*");
			}
			
			System.out.println();
		}
		}
		

	}
	
}
