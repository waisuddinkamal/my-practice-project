package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		int days = 9;

		switch (days) {
		case 1:
			System.out.println("Monday");
			break;
		case 2:
			System.out.println("Tuseday");
			break;
		case 3:
			System.out.println("Wednsday");
			break;
		case 4:
			System.out.println("Thursday");
			break;
		case 5:
			System.out.println("Friday");
			break;
		case 6:
			System.out.println("Saturday");
			break;

		default:
			System.out.println("Sunday");

		}

		double daysOfTheWeek = 4;
		if (daysOfTheWeek == 1) {
			System.out.println("Monday");
		} else if (daysOfTheWeek == 2) {
			System.out.println("Tuseday");
		} else if (daysOfTheWeek == 3) {
			System.out.println("Wednsday");
		} else if (daysOfTheWeek == 4) {
			System.out.println("Thursday");
		} else if (daysOfTheWeek == 5) {
			System.out.println("Friday");
		} else if (daysOfTheWeek == 6) {
		} else {
			System.out.println("Sunday");
		}
			double grade = 0;
			String student = "getName"
		if (grade < 60)
			System.out.println("You are failing.");
			else
			System.out.println("You are passing.");
			// With Curly Braces
			if (student.getName().charAt(0) <= 'M') 
			{
			firstGroup.add(student);
			}
			else
			{
			secondGroup.add(student);
			}
	}

}
