package ClassPractice;

import java.util.Iterator;

public class PracticeAndClean {

	public static void main(String[] args) {
		

		int []a = new int [3];
		
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		
		for(int i = 0; i <a.length;i++) {
			System.out.println(a[i]);
		}
		System.out.println("--------------------------");
		int []b = {10,20,40,50};
		for (int i = 0; i < b.length; i++) {
			System.out.println(a[i]);
			
		}
			
		}
		
	
	}



