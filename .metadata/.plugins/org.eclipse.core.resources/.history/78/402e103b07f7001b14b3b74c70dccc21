package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {
		String name = "kamal";

		int i = name.length() - 1;
		while (i >= 0) {
			System.out.print(name.charAt(i--));

		}
		System.out.println();
		char c = 'a';
		while (c <= 'z') {
			System.out.print(c++ + " ");

		}
		System.out.println();
		char c1 = 'z';
		while (c1 >= 'a') {
			System.out.print(c1-- + " ");
			if (c1 != 'a' - 1) {
				System.out.print(",");
			}

			System.out.println();
			int a = 1;
			while (a <= 50) {
				System.out.println(a++);
			}
			

		}

	}
}
