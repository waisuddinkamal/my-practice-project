package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		char c = 'a';
		while (c <= 'z') {
			System.out.print(c++ + " ");
			if (c != 'z'+1) {
				System.out.print("_");
				
			}
				String s = "kamal";
				int i = s.length()-1;
				while (i>=0) {
					System.out.print(s.charAt(i--));
				}
				
				

			}
		}

	}

