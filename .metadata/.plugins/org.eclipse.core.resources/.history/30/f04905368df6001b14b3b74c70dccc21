package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		for (int i = 1; i <= 7; i++) {
			for (int p = 1; p <= i; p++) {
				System.out.print("*");
				{
					System.out.println();
					for (int i1 = 7; i1 >= 1; i1--) {
						for (int p1 = 7; p1 >= i1; p1--) {
							System.out.println("*");
						}
					}

				}
			}
		}
	}
}
