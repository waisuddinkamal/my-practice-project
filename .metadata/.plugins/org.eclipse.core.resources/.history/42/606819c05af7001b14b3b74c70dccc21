package ClassPractice;

public class PracticeAndClean {

	public static void main(String[] args) {

		String s = "kamal";
		int i = s.length()-1;
		while (i>=0) {
			System.out.print(s.charAt(i--));
			
		}
		System.out.println();
	
		String name = "kamal";
		for(int n = 4;n>=0;) {
			System.out.print(name.charAt(n--));
		}
		

	}

}
